%% Cloudspecs.fyi link style

\usepackage{soul}
\newcommand{\colorref}[3][]{%
  \begingroup
  \sethlcolor{#2}%
  \ifthenelse{\equal{#1}{}}{}{\color{#1}}%
  \ifthenelse{\equal{#2}{}}{\texttt{\textbf{#3}}}{\,\hl{\,\texttt{\textbf{\mbox{#3}}}\,}\,}%
  \endgroup
}

\newcommand{\colorhl}[2]{%
  \begingroup%
  \setlength{\fboxsep}{2pt}\colorbox{#1}{#2}%
  \endgroup%
}


\newcommand*{\fyiLink}{
  \begingroup%
    \urlstyle{tt}%
    \setlength{\fboxsep}{2pt}\colorbox{lightgray!50}{\url{https://cloudspecs.fyi}}%
  \endgroup%
}
