%% Associate names to links so they don't pollute the main document
\newcommand\savedata[2]{\expandafter\def\csname data-#1\endcsname{#2}}
\newcommand\data[1]{\csname data-#1\endcsname}

%% Clickable Links with a link icon using the URL
\newcommand{\clickableLink}[1]{%
  \href{#1}{\includegraphics[width=1em]{resources/fa-chain.pdf}}%
}
%% Clickable Links with a link icon using the associated name
\newcommand{\linkRef}[1]{\clickableLink{\data{#1}}}

%% Clickable Figures
%% Assumes link-name == figure-name under figures/[figure-name].pdf
\newcommand{\linkFigure}[2][width=\linewidth]{%
  \href{\data{#2}}{\includegraphics[#1]{figures/#2.pdf}}%
}

\newcommand{\clickableERCLogo}{%
  \href{\data{erc-logo}}{\includegraphics[width=1.4em]{resources/erc_logo.png}}%
}

